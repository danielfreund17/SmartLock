<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="smartlock.code.Activities.AppInformationActivity">

    <ImageView
        android:id="@+id/ImageBackgroundInfoActivity"
        android:layout_width="385dp"
        android:layout_height="545dp"
        android:background="@drawable/light_wall_background"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteX="-722dp"
        tools:layout_editor_absoluteY="-456dp"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/wings_background"
        tools:layout_editor_absoluteY="38dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="357dp"
        android:layout_height="391dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.357"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.921"
        app:srcCompat="@drawable/app_i"
        tools:layout_editor_absoluteY="107dp"/>

</android.support.constraint.ConstraintLayout>
